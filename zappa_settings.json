        "project_name": "youtube-cutter",
        "runtime": "python3.9",
        "s3_bucket": "youtube-cutter-static-files-dev",
        "keep_warm": false,
        "layers": ["arn:aws:lambda:us-east-1:235154285215:layer:ffmpeg:1"],
